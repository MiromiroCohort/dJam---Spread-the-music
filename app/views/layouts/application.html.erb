<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>DJamSpreadTheMusic</title>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <% if flash.notice %>
      <section class="flash-notice">
        <%= flash.notice %>
      </section>
    <% end %>
    <%= yield %>
  </body>
</html>
